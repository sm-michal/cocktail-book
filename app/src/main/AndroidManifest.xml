<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cocktailbook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.RecipesList"
            android:parentActivityName=".activities.MainActivity">

        </activity>
        <activity
            android:name=".activities.storage.StorageAdditionals"
            android:parentActivityName=".activities.storage.StorageJuices">

        </activity>
        <activity
            android:name=".activities.storage.StorageJuices"
            android:parentActivityName=".activities.storage.StorageOtherAlcohols">

        </activity>
        <activity
            android:name=".activities.storage.StorageOtherAlcohols"
            android:parentActivityName=".activities.storage.StorageBaseAlcohols">

        </activity>
        <activity
            android:name=".activities.storage.StorageBaseAlcohols"
            android:parentActivityName=".activities.MainActivity">

        </activity>
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>