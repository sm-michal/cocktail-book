<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cocktailbook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RecipesList"
            android:parentActivityName=".MainActivity">

        </activity>
        <activity
            android:name=".StorageAdditionals"
            android:parentActivityName=".StorageJuices">

        </activity>
        <activity
            android:name=".StorageJuices"
            android:parentActivityName=".StorageOtherAlcohols">

        </activity>
        <activity
            android:name=".StorageOtherAlcohols"
            android:parentActivityName=".StorageBaseAlcohols">

        </activity>
        <activity
            android:name=".StorageBaseAlcohols"
            android:parentActivityName=".MainActivity">

        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>